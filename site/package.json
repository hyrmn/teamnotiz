{
  "name": "teammotizsite",
  "version": "1.0.0",
  "description": "The marketing site for Team Notiz - a site sharing notes",
  "scripts": {
    "clean": "rimraf ./dist/",
    "build": "eleventy",
    "build-prod": "cross-env NODE_ENV=production npm run clean && npm run build",
