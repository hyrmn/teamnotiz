    "@tailwindcss/typography": "^0.5.2",
    "autoprefixer": "^10.4.4",
    "axios": "^0.26.1",
    "cross-env": "^7.0.3",
    "luxon": "^2.3.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "postcss-import": "^14.1.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.24"
  }
}
